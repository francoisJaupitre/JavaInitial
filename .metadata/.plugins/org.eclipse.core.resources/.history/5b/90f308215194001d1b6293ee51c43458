package test;

import java.util.Calendar;

public class Solution {

	
	public static String findDay(int month, int day, int year) {
		
		Calendar calendar = Calendar.getInstance();
		calendar.set(year, month, day);
		switch(calendar.DAY_OF_WEEK){x
    		case 1: 
    			return "MONDAY";
    			break;
    			
    		case 2: 
    			return "TUESDAY";
    			break;
    		case 3: 
    			return "WEDNESDAY";
    			break;
    		case 4: 
    			return "THURSDAY";
    			break;
    		case 5: 
    			return "FRIDAY";
    			break;
    		case 6: 
    			return "SATURDAY";
    			break;
    		case 7: 
    			return "SUNDAY";
    			break;
    		
    	}
		
	}

    

	public static void main(String[] args){	
    	  
    	System.out.println(findDay(1,14,2023));
		
	}//end of main

}//end of class

